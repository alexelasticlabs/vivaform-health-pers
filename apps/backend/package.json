{
  "name": "@vivaform/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run start:dev",
    "dev:free": "kill-port 4000 && pnpm run dev",
    "dev:nodemon": "kill-port 4000 && nodemon --legacy-watch --watch src --ext ts,js --exec ts-node src/main.ts",
    "admin": "tsx scripts/manage-admin.ts",
    "admin:create": "tsx scripts/manage-admin.ts create",
    "admin:promote": "tsx scripts/manage-admin.ts promote",
    "admin:list": "tsx scripts/manage-admin.ts list",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:debug": "ts-node-dev --respawn --transpile-only --inspect=9229 src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e:prepare": "prisma migrate deploy && prisma db seed",
    "test:e2e:raw": "pnpm run test:e2e:prepare && vitest run --config vitest.e2e.config.ts",
    "test:e2e": "pnpm --filter @vivaform/backend test:e2e:raw",
    "test:e2e:single": "vitest run --config vitest.e2e.config.ts --testNamePattern",
    "format": "prettier --check \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma": "prisma"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "6.18.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/node": "^8.38.0",
    "@sentry/profiling-node": "^8.38.0",
    "@types/nodemailer": "^7.0.3",
    "@vivaform/shared": "workspace:*",
    "argon2": "^0.41.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.6",
    "expo-server-sdk": "^4.0.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.4.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.17.0",
    "stripe": "^16.12.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.8",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.5",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.38",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitest/coverage-v8": "^2.1.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "6.18.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "^2.1.4",
    "vitest-mock-extended": "^1.1.25"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}